<%= form_for(@student) do |f| %>
  <%= f.file_field :profile_photo %></br>
  <%= f.text_field :first_name, placeholder: 'First Name' %></br>
  <%= f.text_field :last_name, placeholder: 'Last Name' %></br>
  <%= f.number_field :age, placeholder: 'Age', min: 1, max:150 %></br>



<p>
    <b>Highest Level of Education</b><br />
    <%= f.radio_button :education, 'High School' %> High School<br />
    <%= f.radio_button :education, 'College' %> College<br />
    <%= f.radio_button :education, 'Masters' %> Masters<br />
    <%= f.radio_button :education, 'PHD' %> PHD<br />
</p>


  <%= f.submit 'Add Student' %>
<% end %>
