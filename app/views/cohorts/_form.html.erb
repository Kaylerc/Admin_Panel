<%= form_for(@cohort) do |f| %>
  Name: <%= f.text_field :name, placeholder: 'Cohort Name', required: true%><br>
  Start Date: <%= f.date_field :start_date, placeholder: 'Start Date', required: true%><br>
  End Date: <%= f.date_field :end_date, placeholder: 'End Date', required: true %><br>
  Course :<%= f.collection_select(:course_id, Course.all, :id, :name, {include_blank: true}) %><br>
  Instructor: <%= f.collection_select(:instructor_id, Instructor.all, :id, :full_name, {include_blank: true}) %><br>

  <%= f.submit %>
  <hb>
      <%= @students %>
<% end %>
