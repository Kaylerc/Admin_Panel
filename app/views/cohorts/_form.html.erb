<%= form_for(@cohort) do |f| %>
  <%= f.text_field :name, placeholder: 'Cohort Name', required: true%><br>
  <%= f.date_field :start_date, placeholder: 'Start Date', required: true%><br>
  <%= f.date_field :end_date, placeholder: 'End Date', required: true %><br>
  <%= f.text_field :course, placeholder: 'Course', required: true%><br>
  <%= f.submit %>
<% end %>
